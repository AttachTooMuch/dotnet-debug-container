# Ignore all log files and temp files
*.log
*.tmp
*.bak

# Ignore system files
.DS_Store
Thumbs.db

# Ignore node_modules and other dependency directories
node_modules/
vendor/

# Ignore Python cache files
__pycache__/
*.py[cod]

# Ignore compiled binaries
*.o
*.out
*.class

# Ignore build directories
build/
dist/

# Ignore sensitive files (adjust according to your use case)
.env
*.env

# Ignore IDE and editor config files
.vscode/
.idea/
*.swp
*.swo
*.swn
*.sublime-project
*.sublime-workspace

# Ignore docker related files that might contain sensitive data
docker-compose.override.yml
docker-compose.override.yaml

# Ignore all executables
*.exe
*.dll

# Ignore coverage reports
coverage/
*.cover
*.lcov
*.lcov-report

# Ignore lock files
package-lock.json
yarn.lock

# Ignore .NET debug container folder
/dotnet-debug-container/image-internals/vsdbg/

# Allow specific files
!*.sh
!Dockerfile
!docker-compose.yml
!docker-compose.yaml
